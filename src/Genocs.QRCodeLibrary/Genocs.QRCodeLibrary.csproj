<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>1.1.1</Version>
    <MinClientVersion>5.0.0</MinClientVersion>
    <Company>[Genocs] Nocco Giovanni Emanuele</Company>
    <Authors>Nocco Giovanni Emanuele</Authors>
    <Title>The base library useful to build .NET Core projects.</Title>
    <Description>QRCode library for .NET Core.</Description>
    <Copyright>Genocs 2021</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/Genocs/qrcode</PackageProjectUrl>
    <PackageIcon>genocs-icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/Genocs/qrcode.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>QRCode reader QRCode writer</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="genocs-icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="6.0.0" />
  </ItemGroup>

</Project>
